#!/usr/bin/env bash

http_proxy="http://127.0.0.1:7890"
https_proxy="http://127.0.0.1:7890"
socket_proxy="socks5://127.0.0.1:7891"
export http_proxy="$http_proxy"
export HTTP_PROXY="$http_proxy"
export https_proxy="$https_proxy"
export HTTPS_PROXY="$https_proxy"
export ALL_PROXY="$socket_proxy"

"$@"
