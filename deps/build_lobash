#!/usr/bin/env bash

set -o errexit
set -o nounset
set -o pipefail
set -o errtrace
(shopt -p inherit_errexit &>/dev/null) && shopt -s inherit_errexit

readonly SCRIPT_DIR="$(cd -P -- "$(dirname -- "$0")" && pwd -P)"

export BASH_MIN_VERSION=4.4

PREFIX=l. lobash-build "$SCRIPT_DIR/lobash.bash"

PREFIX=dotfiles_l. lobash-build "$SCRIPT_DIR/dotfiles_l.bash"
